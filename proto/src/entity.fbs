
namespace proto;

table Vec3 {
  x: float;
  y: float;
  z: float;
}

table Entity {
  id: int; // entity id
  type_id: int; // entity type id
  position: Vec3;
  velocity: Vec3;
  rotation: Vec3;
  scale: Vec3;
  attributes: [ubyte] (flexbuffer);
  children: [Entity];
}

file_identifier "ENTY";

root_type Entity;
